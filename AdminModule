
local AdminModule = {}

AdminModule.adminUsernames = {
    ["uujsuhuhggg176"] = true,
    ["VGBCCTrrbite"] = true,
    ["Kayandy0"] = true
}

-- Function to check if a player is an admin
function AdminModule.isAdmin(player)
    return AdminModule.adminUsernames[player.Name] ~= nil
end

function AdminModule.RemoveAdmin(username)
    if AdminList[username] then
        AdminList[username] = nil
    end
end

return AdminModule
