-- Define section buttons and frames
local sectionFrames = {
    Collect = collectFrame,
    Blatant = blatantFrame
}

local function openSection(sectionName)
    for name, frame in pairs(sectionFrames) do
        frame.Visible = (name == sectionName)
    end
end

collectButton.MouseButton1Click:Connect(function()
    openSection("Collect")
end)

blatantButton.MouseButton1Click:Connect(function()
    openSection("Blatant")
end)

-- Define the X Button to close the main frame
local xButton = Instance.new("TextButton")
xButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red background
xButton.BackgroundTransparency = 0.5
xButton.Position = UDim2.new(1, -30, 0, 0)
