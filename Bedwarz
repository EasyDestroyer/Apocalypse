-- Collect functions
local function collectItems(itemName)
    local currentCFrame = Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    for _, v in pairs(game.Workspace.PickupableItems:FindFirstChild(itemName):GetChildren()) do
        if v then
            Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
            wait(0.5)
            Players.LocalPlayer.Character.HumanoidRootPart.CFrame = currentCFrame
        end
    end
end

diamondButton.MouseButton1Click:Connect(function()
    collectItems("Diamond")
end)

emeraldButton.MouseButton1Click:Connect(function()
    collectItems("Emerald")
end)

collectAllDiamondsButton.MouseButton1Click:Connect(function()
    local diamondFolder = game.Workspace.PickupableItems:FindFirstChild("Diamond")
    if diamondFolder then
        for _, v in pairs(diamondFolder:GetChildren()) do
            collectItems("Diamond")
        end
    end
end)

collectAllEmeraldsButton.MouseButton1Click:Connect(function()
    local emeraldFolder = game.Workspace.PickupableItems:FindFirstChild("Emerald")
    if emeraldFolder then
        for _, v in pairs(emeraldFolder:GetChildren()) do
            collectItems("Emerald")
        end
    end
end)

